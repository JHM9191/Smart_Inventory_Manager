<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.mapper.WarehouseMapper">

	<select id="select" parameterType="String" resultType="warehouse">
		SELECT * FROM WAREHOUSE WHERE warehouse_ID=#{warehouse_ID}
	</select>

	<select id="selectall" resultType="warehouse">
		SELECT * FROM WAREHOUSE ORDER BY warehouse_ID, item_code, item_id
	</select>
	
</mapper>