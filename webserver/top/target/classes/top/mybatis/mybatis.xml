"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="top.vo.UserVO" alias="user" />
		<typeAlias type="top.vo.IngredientVO" alias="ingredient" />
