<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="top.mapper.TruckMapper">
	<select id="select" parameterType="String" resultType="truck">
		SELECT * FROM TRUCK WHERE truckID=#{truckID}
	</select>
	<select id="selectall" resultType="truck">
		SELECT * FROM TRUCK
	</select>
</mapper>